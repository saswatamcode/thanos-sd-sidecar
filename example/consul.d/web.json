{
  "services": [
    {
      "id": "prom0_eu1",
      "name": "prometheus0_eu1",
      "tags": ["eu1"],
      "address": "host.docker.internal",
      "port": 9090,
      "checks": [
        {
          "id": "prom0_eu1_check",
          "name": "prometheus0_eu1_check",
          "http": "http://host.docker.internal:9090/metrics",
          "method": "GET",
          "interval": "10s",
          "timeout": "1s"
        }
      ]
    },
    {
      "id": "prom0_us1",
      "name": "prometheus0_us1",
      "tags": ["us1", "replica0"],
      "address": "host.docker.internal",
      "port": 9091,
      "checks": [
        {
          "id": "prom0_us1_check",
          "name": "prometheus0_us1_check",
          "http": "http://host.docker.internal:9091/metrics",
          "method": "GET",
          "interval": "10s",
          "timeout": "1s"
        }
      ]
    },
    {
